<div class="row justify-content-center" id="pagerow">
    <nav aria-label="Page navigation example" name={{nowpage}} id="page">
        <ul class="pagination justify-content-center" name={{totnum}} id="pagenum">
            {{#each paging}}
            <li id="{{index}}" {{#if cur}}class="page-item active disabled" {{/if}} {{#if unclickable}}
                class="page-item disabled" {{/if}} class="page-item"><a class="page-link" href="#">{{{text}}}</a></li>
            {{/each}}
        </ul>
    </nav>
</div>